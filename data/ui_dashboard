library(shiny)
library(shinydashboard)
 sidebar <- dashboardSidebar(
    sidebarMenu(
      menuItem("Time",tabName = "time",icon=icon("dashboard"),
               menuSubItem("Summary",tabName = "sum1",icon=icon("dashboard")),
               menuSubItem("Seasonality",tabName = "season",icon=icon("dashboard")),
               menuSubItem("Time Comparison",tabName = "subtime",icon=icon("dashboard"))
      ),
      menuItem("Geographical Area",tabName = "geo",icon=icon("th"))
  ))
 body<- dashboardBody(
   tabItems(
     tabItem(tabName = "t"),
     tabItem(tabName="season",
             h2("time1")),
     tabItem(tabName="sum1",
             h2("time")),
     tabItem(tabName="subtime",
             h2("time2")),
     tabItem(tabName = "geo",
             h2("Geographical Area"),
   fluidRow(
     tabBox(height=350,width=400,
            selected="Tab1",
            tabPanel("Summary", "GGMAP"),
            tabPanel("Council District", plotOutput("sumgeo")),
            tabPanel("Request Type", "RT")
     )
   ), hr(), hr()
   )))

shinyApp(ui<-dashboardPage(
   dashboardHeader(title = "Final Project"),
   sidebar,
   body),
   server<-function(input,output){
     })
     
